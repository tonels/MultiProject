{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "string",
      "description": "返回码 0-成功"
    },
    "msg": {
      "type": "string",
      "description": "返回信息"
    },
    "result": {
      "type": "object",
      "properties": {
        "mcnShowVo": {
          "type": "object",
          "properties": {



            "programId": {
              "type": "number",
              "description": "节目ID"
            },
            "contentId": {
              "type": "number",
              "description": "内容ID"
            },
            "assetId": {
              "type": "number",
              "description": "媒资ID"
            },
            "assetName": {
              "type": "number",
              "description": "媒资名称"
            },
            "disposeChannel": {
              "type": "number",
              "description": "输出平台"
            },
            "disposeTime": {
              "type": "string",
              "description": "输出时间"
            },
            "groupName": {
              "type": "string",
              "description": "所属工作组"
            },
            "taskName": {
              "type": "string",
              "description": "所属任务"
            },
            "url": {
              "type": "string",
              "description": "h5页面位置信息"
            }




          },
          "description": "媒资数据",
          "required": [
            "url"
          ]
        }
      },
      "description": "返回数据",
      "required": []
    },
    "success": {
      "type": "boolean",
      "description": "是否成功"
    }
  }
}